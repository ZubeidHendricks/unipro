{
    "version": 2,
    "framework": "laravel",
    "buildCommand": "composer install && npm install && npm run build",
    "devCommand": "php artisan serve",
    "outputDirectory": "public",
    "routes": [
        { "src": "/(.*)", "dest": "/index.php" }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "false"
    }
}